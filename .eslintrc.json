{
    "extends": "google",
    "env": {
        "es6": true
    },
    "parserOptions": {
         "ecmaVersion": 8
    },
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "require-jsdoc": "off",
        "object-curly-spacing": [
            "error",
            "always"
        ],
        "max-len": "off"
    }
}